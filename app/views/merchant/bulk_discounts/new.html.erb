<%= content_tag :h1, "Create New Bulk Discount" %>

<%= form_tag merchant_discounts_path do %>
  <%= label_tag :bulk_quantity %>
  <%= text_field_tag :bulk_quantity %>

  <%= label_tag :percentage_discount %>
  <%= text_field_tag :percentage_discount %>

  <%= submit_tag "Create A Discount" %>
<% end %>
